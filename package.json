{
  "name": "stcn-post-bot",
  "version": "1.0.0",
  "description": "Hello World TypeScript Express.js app",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:connection": "curl http://localhost:3000/test/connection",
    "test:webhook": "curl -X POST http://localhost:3000/test/webhook/user-post",
    "env:check": "node -e \"require('./dist/config/env.js'); console.log('Environment check completed')\""
  },
  "keywords": [
    "typescript",
    "express",
    "hello-world"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
